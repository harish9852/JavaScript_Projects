  <!-- example on JS implementations -->
<!DOCTYPE html>	
<html lang="en">
   <head>
  <style>
    body {
      background-color:#ffccff;
    }
    button {
      background-color:blue;
      color: white;
      padding: 10px;
      border: none;
      cursor: pointer;
    }
    h2 {
      color: red;
    }
  </style>
</head>

    <link  rel="icon"  href="logo.png"/>
  </head>
  <body>
	<h3>demo on inline script</h3>
	<button onclick="alert('button pushed')">Push here</button>
	
	<button onclick="alert('hey, why are pulling me')">Pull here</button>
	
	<h2 ondblclick="alert('double clicked')">Click here</h2>
  </body>
</html>